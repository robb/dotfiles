{
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorStyle": "line-thin",
    "editor.fontFamily": "'SF Mono', 'SF Pro', Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.fontWeight": "300",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 24,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80
    ],
    "editor.showFoldingControls": "always",
    "editor.snippetSuggestions": "top",
    "editor.trimAutoWhitespace": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.ldjson": "json",
        "*.podspec": "ruby",
        "*.swiftinterface": "swift"
    },
    "files.exclude": {
        "**/.build": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.github": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.swiftpm": true,
        "**/.swp": true,
        "**/*.playground/timeline.xctimeline": true,
        "**/*.xcworkspace": true,
        "**/CVS": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "redhat.telemetry.enabled": false,
    "search.useGlobalIgnoreFiles": true,
    "sublimeTextKeymap.promptV3Features": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.env.osx": {},
    "terminal.integrated.fontFamily": "'SF Mono', 'SF Pro', monospace",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontWeight": "400",
    "terminal.integrated.fontWeightBold": "800",
    "terminal.integrated.lineHeight": 1.5,
    "tslint.autoFixOnSave": true,
    "typescript.format.semicolons": "insert",
    "typescript.surveys.enabled": false,
    "window.titleBarStyle": "native",
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#5240C9",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiBrightBlue": "#0000FF",
        "terminal.ansiBrightCyan": "#00E5E5",
        "terminal.ansiBrightGreen": "#68BF49",
        "terminal.ansiBrightMagenta": "#CF57D7",
        "terminal.ansiBrightRed": "#C14D35",
        "terminal.ansiBrightWhite": "#E5E5E5",
        "terminal.ansiBrightYellow": "#B6B248",
        "terminal.ansiCyan": "#00A6B2",
        "terminal.ansiGreen": "#00A600",
        "terminal.ansiMagenta": "#B200B2",
        "terminal.ansiRed": "#BF4C35",
        "terminal.ansiWhite": "#BFBFBF",
        "terminal.ansiYellow": "#999900",
        "terminal.background": "#171717",
        "terminal.foreground": "#FFFFFF"
    },
    "workbench.colorTheme": "Solarized Dark",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.feedback.visible": false,
    "css.format.enable": false,
    "html.format.enable": false,
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    }
}
