[
    {
        "key": "ctrl+cmd+s",
        "command": "editor.action.sortLinesAscending",
        "when": "editorHasSelection"
    },
    {
        "key": "ctrl+cmd+e",
        "command": "editor.action.changeAll",
        "when": "editorFocus"
    },
    {
        "key": "cmd+K",
        "command": "workbench.action.terminal.clear",
        "when": "terminalFocus"
    }
]
